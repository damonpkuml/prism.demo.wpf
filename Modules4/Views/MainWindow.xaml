<Window x:Class="Modules4.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:prism="http://prismlibrary.com/"
        mc:Ignorable="d"
        Title="手动加载模块（通过IModuleManager）" Height="450" Width="800">
    <DockPanel LastChildFill="True">
        <Button Name="BtnLoadModule" DockPanel.Dock="Top" Content="加载模块" Click="BtnLoadModule_OnClick" ></Button>
        <ContentControl prism:RegionManager.RegionName="ContentRegion"></ContentControl>
    </DockPanel>
</Window>
